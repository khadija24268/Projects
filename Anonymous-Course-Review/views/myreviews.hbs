<body>
    <h1>All Reviews</h1>
    <table>
        <thead>
            <tr>
                <th>Course Number</th>
                <th>Course Name</th>
                <th>Semester</th>
                <th>Year</th>
                <th>Professor Name</th>
                <th>Review Comment</th>
            </tr>
        </thead>
        <tbody>
            {{#each mine}}
                <tr>
                    <td>{{courseNumber}}</td>
                    <td>{{courseName}}</td>
                    <td>{{semester}}</td>
                    <td>{{year}}</td>
                    <td>{{professor}}</td>
                    <td>{{review}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</body>
